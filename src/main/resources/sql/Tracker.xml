<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tracker">
	<select id="selectAllTracker" resultType="com.mz.entity.Tracker">
		select * from tracker order by trackerId
	</select>
	<select id="alertOffline" resultType="com.mz.entity.Tracker">
		select * from tracker where trackerState='OFFLINE' order by trackerId
	</select>

</mapper> 